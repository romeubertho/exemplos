{
  "name": "amplify",
  "version": "1.0.0",
  "description": "Initial repo to get your hands on AWS Amplify",
  "main": "index.js",
  "repository": "git@github.com:enfase/exemplos.git",
  "author": "romeubertho <romeubertho@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "delete:node_modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "delete:build": "find . -name \"build\" -exec rm -rf '{}' +",
    "delete:yarn.lock": "find . -name \"yarn.lock\" -exec rm -rf '{}' +",
    "clear": "yarn delete:node_modules && yarn delete:build",
    "build:entities": "yarn workspace @amplify/entities run build",
    "build:connectors": "yarn workspace @amplify/connectors run build",
    "build:dev": "yarn build:entities && yarn build:connectors",
    "install": "yarn build:dev"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/serverless/amplify/backend/function/*/src",
      "packages/serverless/amplify/backend/function/*/lib/nodejs"
    ]
  },
  "devDependencies": {
    "prettier": "^2.3.1"
  }
}